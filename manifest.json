{
	"name": "Chrome v2",
	"version": "0.2",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["scripts/jquery.js","scripts/content.js"]
		}
	],
	"background": {
		"scripts": ["scripts/background.js"]
	},
	"permissions": ["tabs"],
	"browser_action": {
		"default_icon": "images/icon.png",
		"default_popup": "html/popup.html",
		"default_title": "A popup will come here"
	}
}